{
  "name": "ong-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prod": "set NODE_ENV=production&& node ./src/index.js",
    "dev": "set NODE_ENV=development&& nodemon ./src/index.js",
    "test":"mocha ./test/*.test.js",
    "test:org":"mocha -timeout 60000 ./test/organizations.test.js  ",
    "test:watch": "mocha ./test/*.test.js --watch",
    "migrate:prod": "sequelize db:create --env=production && sequelize db:migrate --env=production",
    "migrate:dev": "sequelize db:create --env=development && sequelize db:migrate --env=development",
    "migrate:test": "sequelize db:create --env=test && sequelize db:migrate --env=test",
    "seed:prod": "sequelize db:seed:all --env=production",
    "seed:dev": "sequelize db:seed:all --env=development",
    "seed:test": "sequelize db:seed:all --env=test",
    "delete:dev": "sequelize db:migrate:undo:all --env=development",
    "delete:prod": "sequelize db:migrate:undo:all --env=production",
    "delete:test": "sequelize db:migrate:undo:all --env=test"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.49.0",
    "@sendgrid/mail": "^7.6.0",
    "base64-img": "^1.0.4",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^13.0.1",
    "express": "~4.17.2",
    "google-auth-library": "^7.11.0",
    "helmet": "^5.0.1",
    "http-errors": "~2.0.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.10.0",
    "multer": "^1.4.4",
    "mysql2": "^2.3.3",
    "s3-url-parser": "^1.0.3",
    "sequelize": "^6.13.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "jest": "^27.4.7",
    "mocha": "^9.2.0",
    "nodemon": "^2.0.15",
    "sequelize-cli": "^6.4.1",
    "supertest": "^6.2.2"
  }
}
